// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Actives {
    id String @id @default(uuid())
    category String
    locale String
    date_buy String
    value_buy String
    nf Decimal
    rate Decimal
    description String
    provider String
    life_util String
}
